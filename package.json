  ],
  "author": "Hiroyoshi Takahashi <t.hiroyoshi209@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t-hiroyoshi/react-dnd-item/issues"
  },
  "homepage": "https://github.com/t-hiroyoshi/react-dnd-item#readme",
  "dependencies": {
    "react": "^0.14.2",
    "react-dom": "^0.14.2",
    "react-dnd": "^2.0.2"
  },
  "devDependencies": {
    "babel-core": "^5.0.0",
    "babel-jest": "^5.0.0",
    "babel-loader": "^5.0.0",
